  "private": false,
  "scripts": {
    "start": "tsc && export REACT_ENV='production' && node dist/index.js",
    "build": " react-scripts build",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint --ext .js,.jsx ./src",
